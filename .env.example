# Server configuration
PORT=8000
DATABASE_URL=sqlite:///./app.db

# Security
SECRET_KEY=your_secret_key_here
SSH_PRIVATE_KEY_PATH=

# Telegram
BOT_TOKEN=your_bot_token_here

# Development mode (allows mock auth without Telegram)
DEBUG=true

# CORS Origins (comma-separated JSON style or simple list depending on parser, Pydantic handles JSON list string if configured, or we pass env as json string)
# Pydantic BaseSettings usually expects JSON for list fields
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]

